echo "*.dart linguist-detectable=true" > .gitattributes
echo "*.cmake linguist-detectable=false" >> .gitattributes
echo "*.cpp linguist-detectable=false" >> .gitattributes
echo "*.h linguist-detectable=false" >> .gitattributes
git add .gitattributes
git commit -m "Fix GitHub language detection to Dart"
git push origin main
